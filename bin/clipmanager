import os
import sys

if __name__ == '__main__':
    import clipmanager

    from clipmanager.app import main

    # Some non-standard install paths may need to be added
    sys.path.insert(0, os.path.dirname(clipmanager.__file__))
    sys.path.append('/usr/lib/python2.7/site-packages')

    main()
